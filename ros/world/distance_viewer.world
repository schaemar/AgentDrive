include "S1R.inc"
include "hokuyo.inc"

S1R
(       
  # can refer to the robot by this name
  name "r0"
  pose [ 1.20 2.00 0 150 ]

  hokuyolaser(
    color "gray"
    pose [0.04 0.00 -0.11 0]
  )

  ctrl "viewer (  host 'syrotek.felk.cvut.cz' requires ['laser:0' 'position2d:2' ]) "

  alwayson 1
  localization "gps"
  localization_origin [ 0 0 0 0 ]
  laser_return 0
)


window
(
  size [ 800 800 ] # in pixels
  scale 150.0
     # pixels per meteri
  center [ 1.7 1.8 ]
  rotate [ 0  0 ]


  view (       
    show_data 1              
    show_laser 1
    show_laser_strikes 1
    show_ranger 0
    #show_mines 1
  )
)


